// ◮◮◮ GENERATED BY AURORA ◮◮◮
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Module {
  id         String   @id @default(uuid())
  name       String
  image      String
  created_at DateTime @default(now())
}

model Users {
  id         String   @id @default(uuid())
  email      String
  password   String
  language   String
  name       String
  image      String
  active     Boolean
  score      Int
  created_at DateTime @default(now())
}