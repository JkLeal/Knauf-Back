model Content {
  id          String   @id @default(uuid())
  title       String
  description String
  linkVideo   String?
  linkAudio   String?
  image       String?
  created_at  DateTime @default(now())
  seen        Seen?
}

model Seen {
  id          String   @id @default(uuid())
  content     Content  @relation(fields: [contentId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  contentId   String   @unique
}
