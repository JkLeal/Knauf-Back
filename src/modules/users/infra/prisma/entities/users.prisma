model Users {
  id         String   @id @default(uuid())
  email      String
  password   String
  language   String
  name       String
  image      String
  active     Boolean
  score      Int
  created_at DateTime @default(now())
  seen       Seen[]
}

model Seen {
  id         String   @id @default(uuid())
  user       Users    @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  userId     String
}
